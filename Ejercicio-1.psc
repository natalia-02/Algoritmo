Algoritmo Edad_y_peso
	//Definr variables 
	Definir edad Como Entero;
	Definir peso Como Real;
	//Datos de entrada
	Escribir 'Ingresa tu edad';
	Leer edad;
	Escribir 'Ingresa tu peso';
	Leer peso;
	//Datos de salida
	Escribir 'Tienes ",edad," años';
	Escribir 'Pesas ",peso," kilos ';
FinAlgoritmo
